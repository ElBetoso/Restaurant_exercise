<%= form_with(model: @users) do |form| %>
  <div class="row">
  <div class="col-md-6 col-md-offset-3">
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit %>
  </div>
</div>
</div>
<% end %>
